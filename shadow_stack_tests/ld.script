OUTPUT_ARCH( "loongarch" )
ENTRY( kernel_entry )

SECTIONS
{
#  . = 0x9000000000100000;
  . = 0x02000000;

  .text : {
    . = ALIGN(16);
    *(.text .text.*)
    PROVIDE(etext = .);
  }

  .rodata : {
    . = ALIGN(16);
    *(.srodata .srodata.*)
    . = ALIGN(16);
    *(.rodata .rodata.*)
  }

# to seperate RO/RW segments, new binutils warns for RWX segments
  . = ALIGN (CONSTANT (COMMONPAGESIZE));

  .data : {
    . = ALIGN(16);
    *(.sdata .sdata.*) 
    . = ALIGN(16);
    *(.data .data.*)
  }

  .bss : {
    . = ALIGN(16);
    *(.sbss .sbss.*) 
    . = ALIGN(16);
    *(.bss .bss.*)
  }

  PROVIDE(end = .);
}
